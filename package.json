{
  "name": "m.test",
  "version": "0.0.39",
  "homepage": "https://github.com/ivoputzer/m.test#readme",
  "description": "m(icro)test is a lightweight test runner for node.js written in es6+",
  "author": "Ivo von Putzer Reibegg <ivo.putzer@gmail.com> (https://github.com/ivoputzer)",
  "keywords": [
    "m!cro",
    "m.test",
    "mtest",
    "assert",
    "expect",
    "should",
    "context",
    "describe",
    "it",
    "test",
    "sync",
    "async",
    "recursive",
    "skip",
    "beforeEach",
    "afterEach"
  ],
  "main": "index.js",
  "bin": {
    "m.test": "bin/m.test",
    "mtest": "bin/m.test"
  },
  "man": [
    "man/m.test.1"
  ],
  "engineStrict": true,
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "node": "node",
    "test": "$npm_package_bin_m_test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ivoputzer/m.test.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ivoputzer/m.test/issues"
  },
  "standard": {
    "globals": [
      "test", "beforeEach", "afterEach"
    ]
  },
  "directories": {
    "man": "man"
  }
}
